{
  "name": "lemax10/laravel-enums",
  "description": "Simple Enum implementation for Laravel. With ability to casting for Eloquent models and labels translations via lang files.",
  "keywords": ["laravel", "enum", "eloquent", "lemax10", "laravel-enum"],
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Pyankov",
      "email": "rdlrobot@gmail.com",
      "homepage": "https://github.com/LeMaX10"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
    "illuminate/validation": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
    "illuminate/database": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
    "myclabs/php-enum": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.36 || ^5.7.27 || ^6.5.8"
  },
  "autoload": {
    "psr-4": {
      "LeMaX10\\Enums\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeMaX10\\Enums\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LeMaX10\\Enums\\EnumServiceProvider"
      ]
    }
  }
}
